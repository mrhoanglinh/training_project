<div>
  <% flash.each do |key, value| %>
      <% unless value.blank? %>
          <div class="alert alert-success" id="alert-message">
            <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
            <p><%= value %></p>
          </div>
      <% end %>
  <% end %>
</div>